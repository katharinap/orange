<% if @permitted %>
$("#form").html("<%= escape_javascript(render partial: 'form', locals: { exercise: @exercise }) %>");
$("input.date_picker").datepicker({
  dateFormat: 'yy-mm-dd'
});
$("#form-modal-title").html('<%= exercise_modal_title(@exercise) %>');
$("#form-modal").modal('show');
<% else %>
alert('You do not have permissions to edit this entry');
<% end %>